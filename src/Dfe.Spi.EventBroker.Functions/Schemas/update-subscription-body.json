{
  "properties": {
    "subscriptionId": {
      "type": "string",
      "pattern": "^[a-zA-z0-9]{8}\\-[a-zA-z0-9]{4}\\-[a-zA-z0-9]{4}\\-[a-zA-z0-9]{4}\\-[a-zA-z0-9]{12}$"
    },
    "publisher": {
      "type": "string",
      "minLength": 1
    },
    "eventType": {
      "type": "string",
      "minLength": 1
    },
    "endpointUrl": {
      "type": "string",
      "pattern": "https:\\/\\/.*\\..*"
    }
  },
  "required": [
    "publisher",
    "eventType",
    "endpointUrl"
  ]
}